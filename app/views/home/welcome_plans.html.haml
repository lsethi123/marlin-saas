.row
  .col-md-3
  .col-md-6
    .page-header
      %h1= t '.welcome'
    .lead
      %p
        = "#{t('.hi')} #{current_user.full_name},"
      %br  
      %p= t '.alert'
      %p= t '.message'
      %p
        = t '.question_message'
        = succeed "." do
          = link_to t('.support_team'), ''
      %br
      %p
        %center= link_to t('.create_new_property'), welcome_plans_path(o: :trial), class: 'button btn btn-success btn-lg'
  .col-md-3
